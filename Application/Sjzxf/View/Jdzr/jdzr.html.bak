    function show(target){
        var itemname = $(target).text();//获取当前标签的名称
        $(".list-inline a").removeClass("now");
        $(target).addClass("now");
        additemcontent(itemname);
    }

    function additemcontent(itemname){
        var url = "__URL__/getItemContent";
        $.ajax({
            dataType:'json',
            type:'post',
            url:url,
            data: "iname="+itemname,
            success:function(res){
                $("#itemcontent").replaceWith("<div  id='itemcontent'>"
                        +res.content+
                        "</div>");
            }
        });
    }
</script>
  

